package LocatorsFactory;

public class CartPageLocators {

	public static String checkoutButton = "//a[@class='btn_action checkout_button']";

	public static String removeButton = "//button[text()='Remove']";

	public static String removeButtons = "//button[@class='btn_secondary cart_button']";

	public static String listOfItems = "//div[@class='cart_item']";

	public static String continueShoppingButton = "//a[text()='Continue Shopping']";

}